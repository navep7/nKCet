<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">


    <uses-permission android:name="com.google.android.gms.permission.AD_ID"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/cet_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cet_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.PlayStore">

        <activity android:name=".ReadPaperActivity"></activity>


        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/admob_app_id" />

        <activity
            android:name=".ContactUsActivity"
            android:label="@string/title_activity_contact_us"
            android:theme="@style/Theme.PlayStore.NoActionBar" />
        <activity
            android:name=".YearActivity"
            android:label="@string/title_activity_year"
            android:theme="@style/Theme.PlayStore.NoActionBar" />
        <activity
            android:name=".SubjectActivity"
            android:label="@string/title_activity_subject"
            android:theme="@style/Theme.PlayStore.NoActionBar" />
        <activity android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ResultActivity" />
        <activity
            android:name=".QuestionsActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.PlayStore.NoActionBar" />
    </application>

</manifest>